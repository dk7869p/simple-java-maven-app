pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        
        stage('DEV') {
            steps {
                echo 'Hello World DEV'
            }
        }
    stage('PROD') {
            steps {a
                echo 'Hello World from Production'
            }
        }
    }
    
}
